<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<process xmlns="uri:falcon:process:0.1" xmlns:ns2="uri:falcon:feed:0.1" name="cleanseEmailProcess">
    <tags>owner=ETLGroup</tags>
    <pipelines>churnAnalysisDataPipeline</pipelines>
    <clusters>
        <cluster name="primaryCluster">
            <validity start="2014-02-28T00:00Z" end="2016-03-31T00:00Z"/>
        </cluster>
    </clusters>
    <parallel>1</parallel>
    <order>FIFO</order>
    <timeout>minutes(30)</timeout>
    <frequency>hours(1)</frequency>
    <timezone>UTC</timezone>
    <inputs>
        <input name="input" feed="rawEmailFeed" start="now(0,0)" end="now(0,0)"/>
    </inputs>
    <outputs>
        <output name="output" feed="cleanseEmailFeed" instance="now(0,0)"/>
    </outputs>
    <properties>
        <property name="jobPriority" value="VERY_HIGH"/>
        <property name="queueName" value="etl"/>
    </properties>
    <workflow name="emailCleanseWorkflow" engine="pig" path="/user/ambari-qa/falcon/demo/apps/pig/etl/id.pig"/>
    <retry policy="periodic" delay="minutes(15)" attempts="3"/>
    <ACL owner="ambari-qa" group="users" permission="0755"/>
</process>
