<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:feed xmlns="uri:falcon:process:0.1" xmlns:ns2="uri:falcon:feed:0.1" name="rawEmailFeed" description="Raw customer email feed">
    <ns2:tags>classification=secure,externalSystem=USWestEmailServers</ns2:tags>
    <ns2:frequency>hours(1)</ns2:frequency>
    <ns2:late-arrival cut-off="hours(4)"/>
    <ns2:clusters>
        <ns2:cluster name="primaryCluster" type="source">
            <ns2:validity start="2014-02-28T00:00Z" end="2016-03-31T00:00Z"/>
            <ns2:retention limit="days(90)" action="delete"/>
        </ns2:cluster>
    </ns2:clusters>
    <ns2:locations>
        <ns2:location type="data" path="/user/ambari-qa/falcon/demo/primary/input/enron/${YEAR}-${MONTH}-${DAY}-${HOUR}"/>
        <ns2:location type="stats" path="/none"/>
        <ns2:location type="meta" path="/none"/>
    </ns2:locations>
    <ns2:ACL owner="ambari-qa" group="users" permission="0755"/>
    <ns2:schema location="/none" provider="none"/>
</ns2:feed>
